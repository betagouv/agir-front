{
  "name": "agir-front",
  "private": true,
  "version": "1.8.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format:typescript": "prettier --write \"**/*.+(js|ts|json|tsx|jsx|vue)\"",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test --workers=1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@gouvfr/dsfr": "1.11.1",
    "@hotjar/browser": "1.0.9",
    "@sentry/vue": "7.103.0",
    "chart.js": "4.4.1",
    "js-cookie": "3.0.5",
    "jwt-decode": "4.0.0",
    "pinia": "2.1.7",
    "pinia-plugin-persistedstate": "3.2.1",
    "redaxios": "0.5.1",
    "vue": "3.4.20",
    "vue-chartjs": "5.3.0",
    "vue-matomo": "4.2.0",
    "vue-router": "4.3.0",
    "vue3-directive-shepherd": "1.0.2"
  },
  "devDependencies": {
    "@playwright/test": "1.42.0",
    "@testing-library/vue": "8.0.2",
    "@types/node": "20.11.21",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitest/coverage-v8": "1.3.1",
    "@vue/test-utils": "2.4.2",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.18.1",
    "husky": "9.0.11",
    "jsdom": "22.1.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "14.0.1",
    "prettier": "3.2.5",
    "release-please": "16.3.1",
    "ts-node": "10.9.2",
    "typescript": "5.2.2",
    "validate-branch-name": "1.3.0",
    "vite": "5.1.4",
    "vitest": "1.3.1",
    "vue-tsc": "1.8.22"
  },
  "resolutions": {
    "semver": "7.5.3",
    "strip-ansi": "6.0.1",
    "string-width": "4.2.2",
    "wrap-ansi": "7.0.0"
  },
  "lint-staged": {
    "*.(ts|vue)": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
