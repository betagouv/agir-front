{
  "name": "agir-front",
  "private": true,
  "version": "1.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format:typescript": "prettier --write \"**/*.+(js|ts|json|tsx|jsx|vue)\"",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.10.2",
    "@hotjar/browser": "^1.0.9",
    "@sentry/vue": "^7.81.1",
    "chart.js": "^4.3.0",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^3.1.2",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "redaxios": "^0.5.1",
    "vue": "^3.3.8",
    "vue-chart-3": "^3.1.8",
    "vue-matomo": "^4.2.0",
    "vue-router": "^4.2.5",
    "vue3-directive-shepherd": "^1.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/vue": "^7.0.0",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/test-utils": "^2.4.2",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^14.0.1",
    "prettier": "^2.8.8",
    "release-please": "^16.3.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2",
    "validate-branch-name": "^1.3.0",
    "vite": "^5.0.5",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.21"
  },
  "resolutions": {
    "semver": "7.5.3",
    "strip-ansi": "6.0.1",
    "string-width": "4.2.2",
    "wrap-ansi": "7.0.0"
  },
  "lint-staged": {
    "*.(ts|vue)": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
