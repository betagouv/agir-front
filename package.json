{
  "name": "agir-front",
  "private": true,
  "version": "2.4.0",
  "type": "module",
  "scripts": {
    "convert:webp": "node convertToWebP.js",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format:typescript": "prettier --write \"**/*.+(js|ts|json|tsx|jsx|vue)\"",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test --workers=1",
    "test:e2e--dev": "npx playwright test  --workers=1 --project='chromium' --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postbuild": "node scripts/generate-version.js"
  },
  "dependencies": {
    "@gouvfr/dsfr": "1.13.1",
    "@hotjar/browser": "1.0.9",
    "@iframe-resizer/vue": "^5.4.4",
    "@sentry/vite-plugin": "^3.3.1",
    "@sentry/vue": "9.10.1",
    "@unhead/vue": "^2.0.5",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "chart.js": "4.4.8",
    "jwt-decode": "4.0.0",
    "leaflet": "^1.9.4",
    "marked": "^15.0.8",
    "pinia": "3.0.2",
    "pinia-plugin-persistedstate": "4.2.0",
    "redaxios": "0.5.1",
    "vue": "3.5.13",
    "vue-chartjs": "5.3.2",
    "vue-matomo": "4.2.0",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "1.51.1",
    "@testing-library/vue": "8.1.0",
    "@types/node": "22.13.13",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-vue": "5.2.3",
    "@vitest/coverage-v8": "3.0.5",
    "@vue/test-utils": "2.4.6",
    "dotenv": "16.5.0",
    "eslint": "8.57.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "9.33.0",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "jsdom-global": "3.0.2",
    "lighthouse": "^12.5.1",
    "lint-staged": "15.5.0",
    "playwright-lighthouse": "^4.0.0",
    "prettier": "3.5.3",
    "release-please": "16.18.0",
    "sharp": "^0.34.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "validate-branch-name": "1.3.2",
    "vite": "6.2.6",
    "vitest": "3.0.5",
    "vue-tsc": "2.2.8"
  },
  "resolutions": {
    "semver": "7.5.3",
    "strip-ansi": "6.0.1",
    "string-width": "4.2.2",
    "wrap-ansi": "7.0.0"
  },
  "lint-staged": {
    "*.(ts|vue)": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
